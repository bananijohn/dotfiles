    },
    "modules": [
        "break",
        {
            "type": "custom",
            "format": "┌──────────────── Hardware ────────────────┐",
            "outputColor": "38;2;242;205;205"
        },
        {
            "type": "title",
            "key": " PC",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "cpu",
            "key": "│ ├󰍛CPU",
            "showPeCoreCount": true,
            "format": "{1}",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛 GPU",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "memory",
            "key": "└ └󰍛 Memory",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "custom",
            "format": "└──────────────────────────────────────────┘",
            "outputColor": "38;2;242;205;205"
        },
        "break",
        {
            "type": "custom",
            "format": "┌──────────────── Software ────────────────┐",
            "outputColor": "38;2;242;205;205"
        },
        {
            "type": "os",
            "key": " OS",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "kernel",
            "key": "│ ├ Kernel",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "packages",
            "key": "│ ├󰏖 Packages",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "shell",
            "key": "│ ├ Shell",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "command",
            "key": "│ ├ OS Age",
            "keyColor": "38;2;242;205;205",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        {
            "type": "uptime",
            "key": "└ └ Uptime",
            "keyColor": "38;2;242;205;205"
        },
        "break",
        {
            "type": "de",
            "key": " DE",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "lm",
            "key": "│ ├ LM",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "wm",
            "key": "│ ├ WM",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛 GPU Driver",
            "format": "{3}",
            "keyColor": "38;2;242;205;205"
        },
        {
            "type": "wmtheme",
            "key": "└ └󰉼 Theme",
            "keyColor": "38;2;242;205;205"
        },

        {
            "type": "custom",
            "format": "└─────────────────────────────────────────┘",
            "outputColor": "38;2;242;205;205"
        },
        {
            "type": "custom",
            "format": "               \u001b[90m  \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m "
